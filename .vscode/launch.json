        }
      ],
      "externalConsole": false,
    },
    {
      "name": "tcp server",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/litl2/tcp_server",
      "args": [
        "1",
        "0",
        "1"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [
        {
          "name": "LD_PRELOAD",
          "value": "/nfs/DAL/litl2/lib/server/libalockepfl_original_server.so"
        }
      ],
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "externalConsole": false
    },
    {
      "name": "rdma server",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/litl2/rdma_server",
      "args": [
        "-c",
        "1",
        "-a",
        "10.10.1.1",
        "-t",
        "1",
        "-l",
        "1"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "externalConsole": false
    },
