{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Plot",
      "type": "debugpy",
      "request": "launch",
      "program": "/nfs/DAL/microbench/plot.py",
      "console": "integratedTerminal"
    },
    {
      "name": "Plot1",
      "type": "debugpy",
      "request": "launch",
      "program": "/nfs/DAL/microbench/plot1.py",
      "console": "integratedTerminal"
    },
    {
      "name": "Plot2",
      "type": "debugpy",
      "request": "launch",
      "program": "/nfs/DAL/microbench/plot2.py",
      "console": "integratedTerminal"
    },
    {
      "name": "orig",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/microbench/test_rdma_conn",
      "args": [
        "-t", "1",
        "-d", "1",
        "-m",
        "0",
        "-f", "/nfs/DAL/microbench/results/test_cum.csv",
        "-g", "/nfs/DAL/microbench/results/test_single.csv",
        "-l", "1",
        "-r", "1",
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [
        // {
        //   "name": "LD_PRELOAD",
        //   "value": "${workspaceFolder}/litl2/lib/original/libbackoff_original.so"
        // }
      ],
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "externalConsole": false
    },
{
      "name": "node1",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/microbench/main_disa_litl",
      "args": [
        "-t", "32",
        "-d", "5",
        "-s", "1",
        "-m", "1",
        "-i", "1",
        "-n", "2",
        "-f", "/nfs/DAL/microbench/results/test_tp.csv",
        "-g", "/nfs/DAL/microbench/results/test_lat.csv",
        "-l", "32",
        "-r", "2",
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [
        {
          "name": "LD_PRELOAD",
          "value": "/nfs/DAL/litl2/lib/original/libbackoff_original.so"
        }
      ],
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "externalConsole": false,
    },
    {
      "name": "node2",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/microbench/main_disa_litl",
      "args": [
        "-t", "32",
        "-d", "5",
        "-s", "1",
        "-m", "1",
        "-i", "2",
        "-n", "2",
        "-f", "/nfs/DAL/microbench/results/test_tp.csv",
        "-g", "/nfs/DAL/microbench/results/test_lat.csv",
        "-l", "32",
        "-r", "2",
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [
        {
          "name": "LD_PRELOAD",
          "value": "/nfs/DAL/litl2/lib/original/libbackoff_original.so"
