      ],
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "externalConsole": false,
    },
    {
      "name": "node3",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/microbench/main_disa_",
      "args": [
        "-t", "32",
        "-d", "5",
        "-s", "1",
        "-m", "1",
        "-i", "3",
        "-n", "3",
        "-f", "/nfs/DAL/microbench/results/test_tp.csv",
        "-g", "/nfs/DAL/microbench/results/test_lat.csv",
        "-l", "512",
        "-r", "2",
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [
        {
          "name": "LD_PRELOAD",
