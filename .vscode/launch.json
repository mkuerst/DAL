  "compounds": [
    {
      "name": "tcp_server + clients",
      "configurations": [
        "tcp server",
        "node1"
      ]
    },
    {
      "name": "rdma_server + 1clients",
      "configurations": [
        "rdma server",
        "node1"
      ]
    },
    {
      "name": "rdma_server + 2clients",
      "configurations": [
        "rdma server",
        "node1", "node2"
      ]
    },
    {
      "name": "2 nodes",
      "configurations": [
        "node1", "node2"
      ]
    },
    {
      "name": "3 nodes",
      "configurations": [
        "node1", "node2", "node3"
      ]
    },
  ]
}